       implementation eq
// Theoretical PID controller
parameters
	real Kp = 644.0 {};	// Properional gain
	real Kd = 0.5 {};	// Derivative gain
	real Ki = 3.9 {};	// Integral gain
variables
	real hidden uP, uD, uI;
equations
	uP = Kp * error;
	uD = Kd * ddt(error);
	uI = Ki * int(error);
	steering = uP + uD + uI;
 implementation_end;
